<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/form-validation.css">
    <link rel="stylesheet" href="/css/offcanvas.css">
    <script src="/js/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/form-validation.js"></script>
    <title>부동산 등록</title>
</head>

<body>
    <section id="nav"></section>
    <section id="nav_second"></section>
    <div class="container">
        <div class="py-5 text-center">
            <hr>
            <h2>부동산 등록</h2>
            <hr>
        </div>

        <div class="row">
            <div class="col-md-12 order-md-1">
                <form class="needs-validation" novalidate>

                    <div class="mb-3">
                        <label for="address">Title</label>
                        <input type="text" class="form-control" id="_title" name="_title" placeholder="title" required>
                        <div class="invalid-feedback">
                            Please enter title for realestate.
                        </div>
                    </div>

                    <div class="mb-3">
                        <label for="address">Seller</label>
                        <input type="text" class="form-control" id="_seller" name="_seller" placeholder="Seller" required>
                        <div class="invalid-feedback">
                            This is your address for blockchain.
                        </div>
                    </div>

                    <div class="mb-3">
                        <label for="address">Address</label>
                        <div class="row">
                            <div class="col-md-6 mb-3">
                                <input type="text" class="form-control" id="_locationAddress" name="_locationAddress" placeholder="Address">
                            </div>
                            <div class="col-md-6 mb-3">
                                <input type="button" class="btn btn-primary btn-lg btn-block" onclick="sample5_execDaumPostcode()" value="address search"><br>
                            </div>
                        </div>
                        <div id="map" style="width:100%;height:400px;margin-top:10px;display:none"></div>
                    </div>

                    <div class="mb-3">
                        <label for="address">Value</label>
                        <input type="text" class="form-control" id="_value" name="_value" placeholder="Value" required>
                        <div class="invalid-feedback">
                            This is value for your realestate.
                        </div>
                    </div>
                    
                    <hr class="mb-4">
                    
                    <button class="btn btn-primary btn-lg btn-block" type="submit">register</button>
                </form>
            </div>
        </div>
    </div>
    <input type="hidden" name="mb_id_session" value="<%=mb_id%>">
</body>
<script src="/js/nav.js"></script>
<script src="/js/nav_second.js"></script>
<script src="https://t1.daumcdn.net/mapjsapi/bundle/postcode/prod/postcode.v2.js"></script>
<script src="//dapi.kakao.com/v2/maps/sdk.js?appkey=c00ea0bf28cce3c71be998107d5a7f76&libraries=services"></script>
<script src="/js/registration.js"></script>
</html>